dish: {
    id:string,
    category:string,
    restId:string,
    title:string,
    description: string,
    price:number,
    availible:boolean
}

combo: {
    id:string,
    category:string,
    restId:string,
    title: string,
    description: string,
    dishes:dishes[],
    availible:boolean
    totalPrice:number
}


rest: {
    id:string,
    title:string,
    description:string,
    owner:user
}



model Restaurent {
    id           Int            @id @default(autoincrement())
    name         String         @db.VarChar(100)
    description  String         @db.VarChar(191)
    owner        User           @relation(fields: [owner_id], references: [id], onDelete: Cascade)
    owner_id     Int
    FoodCategory FoodCategory[]
  }
  
  model FoodCategory {
    id            Int        @id @default(autoincrement())
    name          String     @db.VarChar(191)
    restaurent    Restaurent @relation(fields: [restaurent_id], references: [id], onDelete: Cascade)
    restaurent_id Int
    available     Boolean    @default(true)
  }